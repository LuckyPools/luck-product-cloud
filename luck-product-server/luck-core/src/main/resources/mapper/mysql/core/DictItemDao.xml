<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luck.cloud.core.dao.DictItemDao">

    <resultMap id="dictItemMap" type="com.luck.cloud.core.entity.DictItem" autoMapping="true">
    </resultMap>

    <select id="queryList" resultMap="dictItemMap">
        select t.* from sys_dict_item t
        <where>
            t.is_delete = 0
            <if test="id != null and id != ''">
              and t.id = #{id}
            </if>
            <if test="dictCode != null and dictCode != ''">
                and t.dict_code = #{dictCode}
            </if>
        </where>
        order by t.create_date desc
    </select>
</mapper>
